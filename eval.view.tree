$hyoo_js_eval $mol_book2
	Placeholder null
	plugins /
		<= Theme $mol_theme_auto
	bookmark_list? /string
		\
			\// Example Notebook
			\
			\// load external script
			\const {
			\	$mol_compare_deep: compare
			\} = $mol_import.module(
			\	'https://esm.sh/mol_compare_deep'
			\).default
			\
			\// local vars
			\const one = [1]
			\one.push( one )
			\const left =  { a: [1,one], b:    one  }
			\const right = { a:    one,  b: [1,one] }
			\
			\// result
			\compare( left, right )
	pages /
		<= Menu_page $mol_page
			title @ \Bookmarks
			tools /
				<= Clear $mol_link
					arg * code null
					sub /
						<= Clear_icon $mol_icon_plus
				<= Source $mol_link_source
					uri \https://github.com/hyoo-ru/eval.js.hyoo.ru
				<= Lights $mol_lights_toggle
			body /
				<= Menu $mol_list
					rows <= menu /
						<= Menu_link*default $mol_link
							title <= menu_link_title* \
							arg * code <= menu_link_code* \
		<= Code_page $mol_page
			title @ \Code
			tools /
				<= Perf $mol_link_iconed
					title \
					hint @ \Measure Performance
					uri <= perf \
				<= Bookmark $mol_check_icon
					Icon <= Bookmark_icon $mol_icon_bookmark_outline
					checked?val <=> bookmark?val false
					hint @ \Bookmark
				<= Run $mol_check_icon
					Icon <= Run_icon $mol_icon_play
					checked?val <=> run?val true
					hint <= result_label
			body /
				<= Code $mol_textarea
					hint \javascript..
					value?val <=> code?val \
		<= Result_page $mol_page
			title <= result_label @ \Result
			tools /
				<= Results_close $mol_link
					arg * run \false
					sub /
						<= Results_close_icon $mol_icon_cross
			body /
				<= Result $mol_list
					rows <= logs /
	Log* $hyoo_js_eval_log
		values <= log* /

$hyoo_js_eval_log $mol_view
	values /
	sub /
		<= Prefix $mol_view sub / <= prefix \
		<= Dump*0 $hyoo_js_eval_dump
			value <= dump_value* null
			expanded? <=> dump_expanded*? true

$hyoo_js_eval_pair $mol_view
	sub /
		<= Key $hyoo_js_eval_dump
			value <= key null
		<= Suffix $mol_view sub / <= suffix \
		<= Value $hyoo_js_eval_dump
			value <= value null

$hyoo_js_eval_dump $mol_view
	value null
	sub /
		<= Simple $mol_text_code
			text <= simple \
		<= Expand $mol_expander
			expanded?val <=> expanded?val false
			Trigger <= Expand_head $mol_check_expand
				minimal_height 24
				minimal_width 24
				checked?val <=> expanded?val false
				label /
					<= Expand_title $mol_text_code
						text <= expand_title \
			content <= expand_content /
				<= Pair*0 $hyoo_js_eval_pair
					key <= pair_key* null
					suffix <= pair_suffix* null
					value <= pair_value* null
