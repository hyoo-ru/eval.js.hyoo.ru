$hyoo_js_eval $mol_book2
	Placeholder null
	plugins /
		<= Theme $mol_theme_auto
	pages /
		<= Code_page $mol_page
			title @ \Code
			tools /
				<= Source $mol_link_source
					uri \https://github.com/hyoo-ru/eval.js.hyoo.ru
				<= Lights $mol_lights_toggle
				<= Run $mol_check_icon
					Icon <= Run_icon $mol_icon_play
					checked?val <=> run?val true
					hint <= result_label
			body /
				<= Code $mol_textarea
					hint \javascript..
					value?val <=> code?val \
		<= Result_page $mol_page
			title <= result_label @ \Result
			tools /
				<= Results_close $mol_link
					arg * run \false
					sub /
						<= Results_close_icon $mol_icon_cross
			body /
				<= Result $mol_list
					rows <= logs /
	Log!index $hyoo_js_eval_log
		values <= log!index /

$hyoo_js_eval_log $mol_view
	values /
	Dump!index $hyoo_js_eval_dump
		value <= dump_value!index null
		expanded?val <=> dump_expanded!index?val true

$hyoo_js_eval_dump $mol_view
	key null
	suffix \
	value null
	sub /
		<= Key $hyoo_js_eval_dump
			minimal_height 40
			value <= key
			suffix \:
		<= Simple $mol_text_code
			text <= simple \
		<= Expand $mol_expander
			expanded?val <=> expanded?val false
			title <= expand_title \
			content <= expand_content /
	Inner!index $hyoo_js_eval_dump
		key <= inner_key!index null
		value <= inner_value!index null
