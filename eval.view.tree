$hyoo_js_eval $mol_book2
	Placeholder null
	plugins /
		<= Theme $mol_theme_auto
	pages /
		<= Code_page $mol_page
			title @ \Code
			body /
				<= Code $mol_textarea
					hint @ \type something..
					value?val <=> code?val \
		<= Result_page $mol_page
			title @ \Result
			body /
				<= Result $hyoo_js_eval_dump
					value <= result null
					expanded true

$hyoo_js_eval_dump $mol_view
	key null
	suffix \
	value null
	sub /
		<= Key $hyoo_js_eval_dump
			value <= key
			suffix \:
		<= Simple $mol_text_code
			text <= simple \
		<= Expand $mol_expander
			expanded?val <=> expanded?val false
			title <= expand_title \
			content <= expand_content /
	Inner!index $hyoo_js_eval_dump
		key <= inner_key!index null
		value <= inner_value!index null
